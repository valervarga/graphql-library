{
	"defaultSeverity": "error",
	"extends": [
		"tslint:latest"
	],
	"jsRules": {},
	"rules": {
		"no-console": false,
		"quotemark": false,
		"indent": [true, "tabs", 2],
		"object-literal-sort-keys": false,
		"ordered-imports": false,
		"trailing-comma": false,
		"arrow-parens": false
	},
	"rulesDirectory": []
}
